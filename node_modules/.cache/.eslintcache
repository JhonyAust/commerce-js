ommerce_js\\commerce_js\\src\\contexts\\CartContext.jsx",["157","158"],[],"import React, { createContext, useState, useEffect, useContext } from \"react\";\n\nexport const CartContext = createContext();\n\nconst CartProvider = ({ children }) => {\n  // cart state\n  const [cart, setCart] = useState([]);\n  // item amount state\n  const [itemAmount, setItemAmount] = useState(0);\n  // total price state\n  const [total, setTotal] = useState(0);\n\n  useEffect(() => {\n    const total = cart.reduce((accumulator, currentItem) => {\n      return accumulator + currentItem.price * currentItem.amount;\n    }, 0);\n    setTotal(total);\n  });\n\n  // update item amount\n  useEffect(() => {\n    if (cart) {\n      const amount = cart.reduce((accumulator, currentItem) => {\n        return accumulator + currentItem.amount;\n      }, 0);\n      setItemAmount(amount);\n    }\n  }, [cart]);\n\n  // add to cart\n  const addToCart = (product, id) => {\n    const newItem = { ...product, amount: 1 };\n    // check if the item is already in the cart\n    const cartItem = cart.find((item) => {\n      return item.id === id;\n    });\n    if (cartItem) {\n      const newCart = [...cart].map((item) => {\n        if (item.id === id) {\n          return { ...item, amount: cartItem.amount + 1 };\n        } else return item;\n      });\n      setCart(newCart);\n    } else {\n      setCart([...cart, newItem]);\n    }\n  };\n\n  // remove from cart\n  const removeFromCart = (id) => {\n    const newCart = cart.filter((item) => {\n      return item.id !== id;\n    });\n    setCart(newCart);\n  };\n\n  // cleart cart\n  const clearCart = () => {\n    setCart([]);\n  };\n\n  // increase amount\n  const increaseAmount = (id) => {\n    const cartItem = cart.find((item) => item.id === id);\n    addToCart(cartItem, id);\n  };\n\n  // decrease amount\n  const decreaseAmount = (id) => {\n    const cartItem = cart.find((item) => item.id === id);\n    if (cartItem) {\n      const newCart = cart.map((item) => {\n        if (item.id === id) {\n          return { ...item, amount: cartItem.amount - 1 };\n        } else {\n          return item;\n        }\n      });\n      setCart(newCart);\n    }\n    if (cartItem.amount < 2) {\n      removeFromCart(id);\n    }\n  };\n\n  return (\n    <CartContext.Provider\n      value={{\n        cart,\n        addToCart,\n        removeFromCart,\n        clearCart,\n        increaseAmount,\n        decreaseAmount,\n        itemAmount,\n        total,\n      }}\n    >\n      {children}\n    </CartContext.Provider>\n  );\n};\n\nexport default CartProvider;\n","D:\\Programming Charcha\\React Projects\\commerce_js\\commerce_js\\src\\contexts\\SidebarContext.jsx",[],[],"D:\\Programming Charcha\\React Projects\\commerce_js\\commerce_js\\src\\pages\\Home.jsx",["159"],[],"D:\\Programming Charcha\\React Projects\\commerce_js\\commerce_js\\src\\pages\\ProductDetails.jsx",[],[],"D:\\Programming Charcha\\React Projects\\commerce_js\\commerce_js\\src\\components\\Sidebar.jsx",[],[],"D:\\Programming Charcha\\React Projects\\commerce_js\\commerce_js\\src\\components\\Footer.jsx",[],[],"D:\\Programming Charcha\\React Projects\\commerce_js\\commerce_js\\src\\components\\Header.jsx",[],[],"D:\\Programming Charcha\\React Projects\\commerce_js\\commerce_js\\src\\components\\Product.jsx",[],[],"D:\\Programming Charcha\\React Projects\\commerce_js\\commerce_js\\src\\components\\carousel.component.jsx",["160","161"],[],"import { useState } from \"react\";\nimport {\n  BsFillArrowRightCircleFill,\n  BsFillArrowLeftCircleFill,\n} from \"react-icons/bs\";\nlet slides = [\n  \"https://static.independent.co.uk/s3fs-public/thumbnails/image/2017/12/19/15/istock-463173435.jpg\",\n  \"https://static.independent.co.uk/s3fs-public/thumbnails/image/2017/12/19/15/istock-629508490.jpg\",\n  \"https://static.independent.co.uk/s3fs-public/thumbnails/image/2017/12/19/15/istock-614341332.jpg\",\n  \"https://static.independent.co.uk/s3fs-public/thumbnails/image/2017/12/19/16/